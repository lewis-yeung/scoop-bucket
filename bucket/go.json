{
    "$schema": "https://raw.githubusercontent.com/ScoopInstaller/Scoop/master/schema.json",
    "version": "1.25.3",
    "description": "An open source programming language that makes it easy to build simple, reliable, and efficient software.",
    "homepage": "https://go.dev/",
    "license": {
        "identifier": "BSD-3-Clause",
        "url": "https://go.dev/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/go/go1.25.3.windows-amd64.zip",
            "hash": "bc249a599c6fe9d0d4093c363856f6c6320dbbe05e5d5d8818b711fb4a14fc23"
        },
        "32bit": {
            "url": "https://dl.google.com/go/go1.25.3.windows-386.zip",
            "hash": "4d2b1478b2c9bdab69541cef43ad3bde53a755ae06764fdb12d8a6b724908170"
        }
    },
    "extract_dir": "go",
    "env_add_path": "bin",
    "checkver": {
        "url": "https://golang.org/dl/",
        "regex": "go([\\d.]+)\\.windows-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
