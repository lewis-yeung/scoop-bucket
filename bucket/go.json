{
    "$schema": "https://ghproxy.com/https://raw.githubusercontent.com/ScoopInstaller/Scoop/master/schema.json",
    "version": "1.19",
    "description": "An open source programming language that makes it easy to build simple, reliable, and efficient software.",
    "homepage": "https://go.dev/",
    "license": {
        "identifier": "BSD-3-Clause",
        "url": "https://github.com/golang/go/blob/master/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/go/go1.19.windows-amd64.zip",
            "hash": "bcaaf966f91980d35ae93c37a8fe890e4ddfca19448c0d9f66c027d287e2823a"
        },
        "32bit": {
            "url": "https://dl.google.com/go/go1.19.windows-386.zip",
            "hash": "45b80c0aca6a5a1f87f111d375db5afee3ce0a9fd5834041c39116e643ba1df2"
        }
    },
    "extract_dir": "go",
    "env_add_path": "bin",
    "checkver": {
        "url": "https://golang.org/dl/",
        "regex": "go([\\d.]+)\\.windows-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.google.com/go/go$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://dl.google.com/go/go$version.windows-386.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
