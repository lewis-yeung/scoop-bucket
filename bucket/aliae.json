    "architecture": {
        "64bit": {
            "url": "https://github.com/JanDeDobbeleer/aliae/releases/download/v0.20.1/aliae-windows-amd64.exe",
            "hash": "eb0d81d5923e124826297a47345b025643450d2bef026a919730fa4a142860ff"
        },
        "32bit": {
            "url": "https://github.com/JanDeDobbeleer/aliae/releases/download/v0.20.1/aliae-windows-386.exe",
            "hash": "a5296a4b7daaff2acec2c6b869fd44ff8a9be673d6f55887c744815d807f3397"
        },
        "arm64": {
            "url": "https://github.com/JanDeDobbeleer/aliae/releases/download/v0.20.1/aliae-windows-arm64.exe",
            "hash": "8d77dbd0b674a094ff4c35adf82b5b7dc361cb3fe6ef0144544bf67a5ae46a24"
        }
    },
    "installer": {
        "script": [
            "New-Item -ItemType Directory -Path \"$dir\\bin\" -Force | Out-Null",
            "Move-Item -Path \"$dir\\aliae-windows-*.exe\" -Destination \"$dir\\bin\\aliae.exe\" -Force"
        ]
    },
    "env_add_path": "bin",
    "checkver": {
        "github": "https://github.com/JanDeDobbeleer/aliae"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/JanDeDobbeleer/aliae/releases/download/v$version/aliae-windows-amd64.exe"
            },
            "32bit": {
                "url": "https://github.com/JanDeDobbeleer/aliae/releases/download/v$version/aliae-windows-386.exe"
            },
            "arm64": {
                "url": "https://github.com/JanDeDobbeleer/aliae/releases/download/v$version/aliae-windows-arm64.exe"
            }
        }
    }
}
