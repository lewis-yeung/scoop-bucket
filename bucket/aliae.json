    "architecture": {
        "64bit": {
            "url": "https://github.com/JanDeDobbeleer/aliae/releases/download/v0.20.0/aliae-windows-amd64.exe",
            "hash": "ecfef4fdb910444309e45fdacf9952a8a6ba5c09c396a7a1fcf17706561a1741"
        },
        "32bit": {
            "url": "https://github.com/JanDeDobbeleer/aliae/releases/download/v0.20.0/aliae-windows-386.exe",
