        "- Sample themes can be found in `$dir\\themes`."
    ],
    "architecture": {
        "64bit": {
            "url": [
                "https://ghproxy.com/https://github.com/JanDeDobbeleer/oh-my-posh/releases/download/v12.3.0/posh-windows-amd64.exe",
                "https://ghproxy.com/https://github.com/JanDeDobbeleer/oh-my-posh/releases/download/v12.3.0/themes.zip"
            ],
            "hash": [
                "bcd9419c42119282bec3890dc0ff06dbede17695449fb64e93e0f737104cbf1c",
                "31d04d0f7a64d13028587c37d0ece0c765510a6b5336b391387ca70107f8b7e9"
            ]
        },
        "32bit": {
            "url": [
                "https://ghproxy.com/https://github.com/JanDeDobbeleer/oh-my-posh/releases/download/v12.3.0/posh-windows-386.exe",
                "https://ghproxy.com/https://github.com/JanDeDobbeleer/oh-my-posh/releases/download/v12.3.0/themes.zip"
            ],
            "hash": [
                "8a501337bca6a542a9a2993d2fc693dfc011d1994f33530a4beebf07b3cb9e9b",
                "31d04d0f7a64d13028587c37d0ece0c765510a6b5336b391387ca70107f8b7e9"
            ]
        }
    },
    "extract_to": "themes",
    "installer": {
        "script": [
            "New-Item -ItemType Directory -Path \"$dir\\bin\" -Force | Out-Null",
            "$(if ($architecture -eq '64bit') {",
            "  \"posh-windows-amd64.exe\"",
            "} else {",
            "  \"posh-windows-386.exe\"",
            "}) | ForEach-Object { Move-Item -Path \"$dir\\$_\" -Destination \"$dir\\bin\\oh-my-posh.exe\" -Force }"
        ]
    },
    "env_add_path": "bin",
    "env_set": {
        "POSH_THEMES_PATH": "$dir\\themes"
    },
    "checkver": {
        "github": "https://github.com/JanDeDobbeleer/oh-my-posh"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": [
                    "https://ghproxy.com/https://github.com/JanDeDobbeleer/oh-my-posh/releases/download/v$version/posh-windows-amd64.exe",
                    "https://ghproxy.com/https://github.com/JanDeDobbeleer/oh-my-posh/releases/download/v$version/themes.zip"
                ]
            },
            "32bit": {
                "url": [
                    "https://ghproxy.com/https://github.com/JanDeDobbeleer/oh-my-posh/releases/download/v$version/posh-windows-386.exe",
                    "https://ghproxy.com/https://github.com/JanDeDobbeleer/oh-my-posh/releases/download/v$version/themes.zip"
                ]
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
